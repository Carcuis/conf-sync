{
    "extensions.updateCheck": "weekly",
    "semanticTokens.enable": true,
    "hover.floatConfig": {
        "border": true,
        "winblend": 10,
        "borderhighlight": "FloatBorder"
    },
    "suggest.noselect": false,
    "suggest.enablePreselect": true,
    "suggest.completionItemKindLabels": {
        "method": "",
        "function": "󰊕",
        "variable": "",
        "field": "",
        "typeParameter": "󰊄",
        "constant": "󰏿",
        "class": "",
        "interface": "",
        "struct": "",
        "event": "",
        "operator": "󰆕",
        "module": "",
        "property": "",
        "enum": "",
        "reference": "󰶭",
        "keyword": "",
        "file": "󰈔",
        "folder": "󰉋",
        "color": "",
        "unit": "",
        "snippet": "",
        "text": "󰉿",
        "constructor": "",
        "value": "󰎠",
        "enumMember": "",
        "default": ""
    },
    "suggest.snippetIndicator": " 󰇘",
    "suggest.labelMaxLength": 70,
    "suggest.floatConfig": {
        "border": true,
        "winblend": 10,
        "borderhighlight": "FloatBorder"
    },
    "suggest.pumFloatConfig": {
        "border": false,
        "winblend": 10,
    },
    "suggest.formatItems": [
        "kind",
        "menu",
        "abbr",
        "shortcut"
    ],
    "suggest.virtualText": false,
    "diagnostic.signPriority": 0,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextPrefix": " 󱓻 ",
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.hintSign": "󰌶",
    "diagnostic.errorSign": "",
    "diagnostic.floatConfig": {
        "border": true,
        "winblend": 10,
        "borderhighlight": "FloatBorder"
    },
    "signature.floatConfig": {
        "border": true,
        "winblend": 10,
        "borderhighlight": "FloatBorder"
    },
    "dialog.floatBorderHighlight": "CocHintFloat",
    "[json]": {
        "coc.preferences.formatOnSave": false
    },
    "[ps1][python]": {
        "semanticTokens.enable": false
    },
    "coc.preferences.messageDialogKind": "notification",
    "coc.source.nerdfonts.filetypes": [
        "json",
        "lua",
        "markdown",
        "vim",
        "zsh",
    ],
    "notification.border": false,
    "notification.highlightGroup": "CocNotification",
    "notification.marginRight": 5,
    "notification.statusLineProgress": false,
    "notification.timeout": 5000,
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "tree.closedIcon": "",
    "tree.openedIcon": "",
    "tree.key.close": "q",
    "tree.key.toggle": "o",
    "outline.previewWinblend": 10,
    "outline.previewBorderHighlightGroup": "FloatBorder",
    "outline.previewHighlightGroup": "NormalFloat",
    "outline.autoPreview": false,
    "outline.autoWidth": false,
    "outline.expandLevel": 0,
    "outline.sortBy": "position",
    "outline.detailAsDescription": false,
    "outline.showLineNumber": false,
    "outline.followCursor": false,
    "snippets.ultisnips.enable": true,
    "languageserver": {
        "powershell": {
            "command": "pwsh",
            "args": [
                "-NoLogo",
                "-NoProfile",
                "-NonInteractive",
                "-Command",
                "Import-Module $env:NVIM_DATA_PATH/mason/packages/powershell-editor-services/PowerShellEditorServices/PowerShellEditorServices.psd1;",
                "Start-EditorServices",
                "-HostName coc.nvim",
                "-HostProfileId coc.nvim",
                "-SessionDetailsPath $env:NVIM_DATA_PATH/mason/packages/powershell-editor-services/PowerShellEditorServices/session.json",
                "-AdditionalModules @()",
                "-FeatureFlags @()",
                "-Stdio",
            ],
            "filetypes": [
                "ps1",
                "psm1",
                "psd1",
            ],
            "maxRestartCount": 1,
            "settings": {
                "powershell.codeFormatting.ignoreOneLineBlock": true,
                "powershell.codeFormatting.newLineAfterCloseBrace": false,
                "powershell.codeFormatting.openBraceOnSameLine": true,
            },
        },
        "toml": {
            "command": "tombi",
            "args": [
                "lsp",
            ],
            "filetypes": [
                "toml",
            ]
        }
    },
    "codeLens.enable": true,
    "codeLens.position": "eol",
    "markdownlint.onOpen": false,
    "markdownlint.onChange": false,
    "markdownlint.config": {
        "code-block-style": false,
        "line_length": false,
        "no-emphasis-as-heading": false,
        "no-inline-html": false,
    },
    "markdown-preview-enhanced.codeBlockTheme": "darcula.css",
    "markdown-preview-enhanced.previewTheme": "one-dark.css",
    "translator.window.enableBorder": false,
    "translator.engines": [
        "bing",
        "google",
        "youdao",
        "haici"
    ],
    "sumneko-lua.enableNvimLuaDev": true,
    "sumneko-lua.locale": "zh-cn",
    "Lua.completion.callSnippet": "Replace",
    "Lua.format.defaultConfig": {
        "align_array_table": "false",
    },
    "Lua.runtime.version": "Lua 5.1",
    "basedpyright.analysis.typeCheckingMode": "standard",
    "bashIde.shfmt.caseIndent": true,
    "bashIde.shfmt.spaceRedirects": true,
}
