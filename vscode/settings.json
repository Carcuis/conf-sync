{
    "[code-runner-output]": {
        "editor.fontSize": 14,
        "editor.guides.indentation": false
    },
    "[markdown]": {
        "editor.wordWrap": "off",
    },
    "bashIde.shfmt.caseIndent": true,
    "bashIde.shfmt.spaceRedirects": true,
    "chat.renderRelatedFiles": true,
    "code-runner.executorMap": {
        "java": "javac -encoding utf8 -d class $fileName && java -Dfile.encoding=UTF-8 -cp class $fileNameWithoutExt",
        "python": "set PYTHONIOENCODING=utf8 && $pythonPath $fullFileName",
    },
    "code-runner.respectShebang": false,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "csv-preview.formatValues": "never",
    "debug.openDebug": "openOnDebugBreak",
    "debug.showBreakpointsInOverviewRuler": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.bracketPairColorization.enabled": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "CaskaydiaCove NFM, Microsoft Yahei UI, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.inlayHints.fontFamily": "Calibri",
    "editor.inlayHints.padding": true,
    "editor.largeFileOptimizations": false,
    "editor.lineHeight": 1.6,
    "editor.lineNumbers": "relative",
    "editor.minimap.showSlider": "always",
    "editor.renderLineHighlight": "all",
    "editor.rulers": [ 120 ],
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "off",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "files.associations": {
        ".env": "shellscript"
    },
    "files.defaultLanguage": "",
    "files.simpleDialog.enable": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.enable": {
        "plaintext": true,
        "markdown": true,
        "scminput": true
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.nextEditSuggestions.fixes": true,
    "inlineChat.accessibleDiffView": "on",
    "Lua.completion.callSnippet": "Replace",
    "Lua.format.defaultConfig": {
        "align_array_table": "false"
    },
    "Lua.hint.enable": true,
    "Lua.workspace.library": [
        "D:\\dev\\scoop\\apps\\neovim\\current\\share\\nvim\\runtime"
    ],
    "leetcode.defaultLanguage": "python3",
    "leetcode.editor.shortcuts": [ "description", "solution", "submit", "test", ],
    "leetcode.endpoint": "leetcode",
    "leetcode.hint.commandShortcut": false,
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.workspaceFolder": "D:\\code\\leetcode",
    "markdown.preview.fontSize": 18,
    "markdownlint.config": {
        "code-block-style": false,
        "line_length": false,
        "no-inline-html": false,
    },
    "notebook.inlineValues": "on",
    "notebook.lineNumbers": "on",
    "outline.showVariables": false,
    "output.smartScroll.enabled": false,
    "powershell.codeFolding.showLastLine": false,
    "powershell.codeFormatting.alignPropertyValuePairs": false,
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.integratedConsole.showOnStartup": false,
    "python.analysis.aiHoverSummaries": true,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.languageServerMode": "full",
    "python.analysis.typeCheckingMode": "standard",
    "python.terminal.activateEnvironment": false,
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "host1": "linux"
    },
    "terminal.integrated.allowChords": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.scrollback": 100000,
    "terminal.integrated.smoothScrolling": true,
    "trailing-spaces.backgroundColor": "rgba(255,255,255,0.15)",
    "trailing-spaces.borderColor": "rgba(255,255,255,0.08)",
    "trailing-spaces.highlightCurrentLine": false,
    "vscode-neovim.compositeKeys": {
        "jj": { "command": "vscode-neovim.escape", },
        "jk": { "command": "vscode-neovim.escape", },
        "kj": { "command": "vscode-neovim.escape", },
        "kk": { "command": "vscode-neovim.escape", },
        "jl": { "command": "vscode-neovim.escape", },
        "jh": { "command": "vscode-neovim.escape", },
    },
    "vscode-neovim.ctrlKeysForInsertMode": [ "a", "c", "d", "e", "h", "j", "m", "o", "r", "t", "u", "w" ],
    "vscode-neovim.ctrlKeysForNormalMode": [
        "a", "b", "c", "d", "e", "f", "h", "i", "j", "k", "l", "m", "n", "o", "p", "r", "t", "u", "v", "w",
        "x", "y", "z", "/", "]", "right", "left", "up", "down", "backspace", "delete"
    ],
    "vscode-neovim.NVIM_APPNAME": "vscnvim",
    "vscode-neovim.neovimInitVimPaths.win32": "~\\AppData\\Local\\vscnvim\\init.lua",
    "vsintellicode.python.completionsEnabled": false,
    "window.enableMenuBarMnemonics": false,
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#262626",
        "editorHoverWidget.background": "#202020ee",
    },
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.smoothScrolling": true,
    "workbench.localHistory.maxFileEntries": 500,
    "workbench.panel.showLabels": false,
    "workbench.secondarySideBar.showLabels": false,
}
